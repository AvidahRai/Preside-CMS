<?xml version="1.0" encoding="UTF-8"?>
<form i18nBaseUri="formbuilder.item-types.dateRange:">
	<tab id="default">
		<fieldset id="default">
			<field name="toDate" control="textinput" required="true" sortorder="25">
				<rule validator="match" message="formbuilder.item-types.formfield:validation.error.invalid.name.format">
					<param name="regex" value="^[a-zA-Z][a-zA-Z0-9_]*$" />
				</rule>
			</field>

			<field name="toDateLabel" control="textinput" required="true" sortorder="27" defaultvalue="" />
		</fieldset>
	</tab>
</form>